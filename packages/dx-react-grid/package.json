{
  "name": "@synconset/dx-react-grid",
  "version": "4.0.15",
  "description": "Composable plugin-based data grid component for React",
  "author": {
    "name": "Synconset Inc.",
    "url": "https://www.synconset.com/"
  },
  "bugs": {
    "url": "https://github.com/wymsee/devextreme-reactive/issues"
  },
  "homepage": "https://synconset.github.io/synconset-reactive/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wymsee/devextreme-reactive"
  },
  "keywords": [
    "react",
    "data",
    "grid",
    "table",
    "react-component"
  ],
  "license": "SEE LICENSE IN README.md",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/dx-react-grid.d.cts",
        "default": "./dist/dx-react-grid.umd.cjs"
      },
      "import": {
        "types": "./dist/dx-react-grid.d.ts",
        "default": "./dist/dx-react-grid.es.js"
      },
      "default": "./dist/dx-react-grid.es.js"
    }
  },
  "main": "dist/dx-react-grid.umd.cjs",
  "module": "dist/dx-react-grid.es.js",
  "globalName": "Synconset.DXReactGrid",
  "files": [
    "dist"
  ],
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  },
  "types": "dist/dx-react-grid.d.ts",
  "typings": "dist/dx-react-grid.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage",
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "update-api": "api-extractor run",
    "dts": "tsc -p tsconfig.dts.json && node ./merge-dts.js",
    "lint": "tslint -p tsconfig.lint.json",
    "lint:fix": "yarn lint -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-replace": "^2.3.1",
    "@synconset/dx-core": "^4.0.12",
    "@synconset/dx-react-core": "^4.0.12",
    "@synconset/dx-testing": "^4.0.12",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "core-js": "^3.4.1",
    "enzyme": "3.10.0",
    "enzyme-to-json": "^3.4.3",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^1.27.2",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@synconset/dx-grid-core": "^4.0.14"
  },
  "peerDependencies": {
    "@synconset/dx-core": ">=4.0.0 <5",
    "@synconset/dx-react-core": ">=4.0.0 <5",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
